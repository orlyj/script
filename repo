from github import Github

# GitHub credentials
github_username = 'orlyj'
github_password = '0x3c2a33Bf59F352b54292F3927F3B376e0D99d351'

# Repository information
repo_name = 'new-repository'
repo_description = 'This is a new repository created using PyGithub'

# Create a GitHub instance
g = Github(github_username, github_password)

# Create a new repository
user = g.get_user()
repo = user.create_repo(repo_name, description=repo_description)

print(f"Repository '{repo_name}' created successfully.")
